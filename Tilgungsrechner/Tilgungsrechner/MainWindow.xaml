<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tilgungsrechner"
        xmlns:ViewModel="clr-namespace:Tilgungsrechner.ViewModel"
    xmlns:view="clr-namespace:Tilgungsrechner.View" x:Class="Tilgungsrechner.MainWindow"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" d:DataContext="{d:DesignInstance {x:Type ViewModel:MainWindowViewModel}}">
    <view:TilgungsView DataContext="{Binding TilgungsViewModel, Mode=OneWay}" />
</Window>
